<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /admin
    RewriteRule ^admin/index\.html$ - [L]
    RewriteCond %{REQUEST_URI} ^/admin/
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . /admin/index.html [L]
</IfModule>
